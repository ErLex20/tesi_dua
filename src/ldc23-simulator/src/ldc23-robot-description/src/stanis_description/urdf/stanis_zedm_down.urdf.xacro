    </visual>
  </link>
  <gazebo reference="d435i_link">
    <collision>
      <surface>
        <contact>
          <ode>
            <min_depth>${min_depth}</min_depth>
            <max_vel>${max_vel}</max_vel>
          </ode>
        </contact>
      </surface>
    </collision>
  </gazebo>
  <joint name="d435i_joint" type="fixed">
    <parent link="base_link"/>
    <child link="d435i_link"/>
    <origin rpy="0 -${zedm_tilt} 0" xyz="${d435i_to_base_x} ${d435i_to_base_y} ${d435i_to_base_z}"/>
  </joint>
  <xacro:fixed_joint_params joint="d435i_joint"/>
  <link name="d435i_depth_link">
    <xacro:eps_inertial/>
  </link>
  <gazebo reference="d435i_depth_link">
    <sensor name="d435i_depth_camera" type="depth">
      <update_rate>${d435i_depth_update_rate}</update_rate>
      <topic>${d435i_depth_topic}</topic>
