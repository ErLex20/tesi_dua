  <xacro:property name="bottomcam_to_base_z" value = "-0.155"/>
  <xacro:property name="bottomcam_tilt" value = "${pi/2}"/>

  <xacro:property name="laser_to_base_x" value = "0.06"/>
  <xacro:property name="laser_to_base_y" value = "0"/>
