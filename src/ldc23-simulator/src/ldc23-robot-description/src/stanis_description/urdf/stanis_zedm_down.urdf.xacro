        <horizontal_fov>${d435i_depth_hfov}</horizontal_fov>
        <image>
          <width>${d435i_depth_width}</width>
          <height>${d435i_depth_height}</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>${d435i_depth_clip_near}</near>
          <far>${d435i_depth_clip_far}</far>
        </clip>
      </camera>
      <plugin name="d435i_depth_camera" filename="libgazebo_ros_camera.so">
